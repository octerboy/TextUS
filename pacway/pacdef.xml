<?xml version="1.0" encoding="GB2312" ?>
<instructions>
	<!--send recv 是保留字, 自定义指令中slot component reply sw是任意的, 属性from to error not note是保留的,  -->
	<!-- error属性,指recv或END_SESSION或报文错误. 应用错误不在这里定义 -->
 	<hsm subor="2" error="0A">
		<send field="0" note="这里设置两字节, 预留长度空间, 那个aux的unifom就不要了">99</send>
		<head to="2" note="HSM指令头"/>
		<component to="3" note="HSM指令内容，不包括指令头"/>
		<status from="2" note="HSM指令响应头"/>
		<reply from="3"/>
 	</hsm>

	<icc subor="1" error="0B" is_icc="true">
		<send field="0" note="这里设置两字节, 预留长度空间, 那个aux的unifom就不要了">99</send>
		<send field="1" note="表示这是APDU指令">C</send>
		<send to="2" note="卡槽" >0</send>
		<component to="3" note="各元素内容合并就是指令内容" />
		<recv field="1" error="11" note="APDU指令响应标志">c</recv>
		<recv field="3" error="0E" note="指令返回正常标志">0</recv>
		<reply from="4" note="指令响应内容"/>
		<sw from="5" note="指令状态字"/>
	</icc>

	<random subor="1" error="0B" is_icc="true">
		<send field="0" note="这里设置两字节, 预留长度空间, 那个aux的unifom就不要了">99</send>
		<send field="1" note="表示这是APDU指令">C</send>
		<send to="2" note="卡槽" >0</send>
		<send to="3" >0084000008</send>
		<recv field="1" error="11" note="APDU指令响应标志">c</recv>
		<recv field="3" error="0E" note="指令返回正常标志">0</recv>
		<reply from="4" note="指令响应内容"/>
		<recv field="5" error="0E" >9000</recv>
	</random>

	<sran subor="1" error="0B" is_icc="true">
		<send field="0" note="这里设置两字节, 预留长度空间, 那个aux的unifom就不要了">99</send>
		<send field="1" note="表示这是APDU指令">C</send>
		<send to="2" note="卡槽" >0</send>
		<send to="3" >0084000004</send>
		<recv field="1" error="11" note="APDU指令响应标志">c</recv>
		<recv field="3" error="0E" note="指令返回正常标志">0</recv>
		<reply from="4" note="指令响应内容"/>
		<recv field="5" error="0E" >9000</recv>
	</sran>

	<reset subor="1" error="0C">
		<send field="0" note="这里设置两字节, 预留长度空间, 那个aux的unifom就不要了">99</send>
		<send field="1" note="表示这是复位指令">R</send>
		<slot to="2" note="卡槽" />
		<recv field="1" error="11" note="复位指令响应标志">r</recv>
		<recv field="3" error="0C" note="指令返回正常标志">0</recv>
		<result from="4" note="结果代码"/>
		<face from="5" note="界面"/>
		<atr from="6" note="复位信息"/>
		<uid from="7" note="UID"/>
		<err from="8" note="错误描述"/>
	</reset>

	<feed subor="1" error="0D">
		<send field="0" note="这里设置两字节, 预留长度空间, 那个aux的unifom就不要了">99</send>
		<send field="1" note="表示这是进卡">P</send>
		<slot to="2" note="卡槽" />
		<card to="3" note="卡号" />
		<flag to="4" note="厂商标志" >0</flag>
		<description to="5" note="厂商描述" />
		<recv field="1" error="11" note="指令响应标志">p</recv>
		<recv field="2" error="0E" note="指令返回正常标志">0</recv>
	</feed>

	<out subor="1">
		<send field="0" note="这里设置两字节, 预留长度空间, 那个aux的unifom就不要了">99</send>
		<send field="1" note="表示这是出卡">O</send>
		<slot to="2" note="卡槽" />
		<stat to="3" note="制卡是否成功标志" />
		<description to="4" note="卡号" />
		<recv field="1" error="11" note="响应标志">o</recv>
		<recv field="2" error="0E" note="返回正常标志">0</recv>
	</out>
	<prompt subor="1">
		<send field="0" note="这里设置两字节, 预留长度空间, 那个aux的unifom就不要了">99</send>
		<send field="1" note="表示这是提示">P</send>
		<slot to="2" note="卡槽" />
		<many to="3" note="指令总数" />
		<recv field="1" error="11" note="响应标志">p</recv>
		<recv field="2" error="0E" note="返回正常标志">0</recv>
	</prompt>
</instructions>
