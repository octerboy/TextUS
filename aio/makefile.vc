include ..\Rules.basemod.vc
CFLAGS =$(CFLAGS) -I..\logdata\include  -I..\baselib\include
LDLIB =$(LDLIB) ..\baselib\libbase.lib
AMOROBJS=Aio.obj

all: aio.dll

Aio.obj: Aio.cpp
	$(CC) $(CFLAGS) -I..\tpoll\include -DNNDEBUG -DAMOR_CLS_TYPE=Aio Aio.cpp

aio.dll: $(AMOROBJS)
	$(LD) $(LDFLAGS) -out:$@ $(AMOROBJS)  $(LDLIB)
	$(_VC_MANIFEST_PRO)
	$(_POST_RPO_SUB)
	
clean:
	del *.obj
	del *.dll.*
	del *.exp
	del *.lib
